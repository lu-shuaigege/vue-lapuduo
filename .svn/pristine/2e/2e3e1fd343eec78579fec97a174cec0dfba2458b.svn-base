'';\n        this.verificationCode = '';\n        this.cancelFn()\n      },\n      //验证\n      checkFormData() {\n        if (this.userName=='') {\n          this.$alert('请输入账号', {\n            confirmButtonText: '确定',\n          });\n          this.checkFormDataRuleForm = false\n          return\n        }\n        if (this.pwd=='') {\n          this.$alert('请输入密码', {\n            confirmButtonText: '确定',\n          });\n          this.checkFormDataRuleForm = false\n          return\n        }\n        if (this.verificationCode=='') {\n          this.$alert('请输入验证码', {\n            confirmButtonText: '确定',\n          });\n          this.checkFormDataRuleForm = false\n          return\n        }\n        this.checkFormDataRuleForm = true\n      },\n\n  },\n    components: {\n    }\n  };\n</script>\n<style lang=\"scss\" scoped>\n  .content{\n    position: relative;\n    width: 100%;\n    background-image: url(../../assets/images/开放平台-1.jpg);\n    background-size:100% 100%;\n    background-repeat:no-repeat;\n    height:500px;\n    .qrcode{\n      position: absolute;\n      top: 13%;\n      right: 27%;\n      background-image: url(../../assets/images/开放平台-8.png);\n      width: 570px;\n      height: 400px;\n\n\n    }\n  }\n\n  .submit_case {\n    padding: 40px 0;\n    height: 288px;\n    margin: 0 auto;\n    display: flex;\n    flex-direction: column;\n    justify-content: space-around;\n  }\n\n  .item {\n    width: 417px;\n    margin: 0 auto;\n    display: flex;\n    flex-wrap: nowrap;\n  }\n\n  .item .el-select {\n    width: 100%;\n  }\n\n  .code_img {\n    height: 40px;\n    width: 100px;\n    display: block;\n    margin: 0 0 0 10px;\n    cursor: pointer;\n  }\n\n  .admin_title {\n    text-align: center;\n    color: #fff;\n    font-size: 16px;\n    -moz-user-select: none;\n    -khtml-user-select: none;\n    user-select: none;\n  }\n\n  .radio_login {\n    padding: 10px 0;\n    text-align: center;\n    margin: 0 auto;\n  }\n\n  .radio_login .el-radio {\n    color: #fff !important;\n  }\n\n\n</style>\n\n\n\n\n// WEBPACK FOOTER //\n// src/views/openArea/openArea.vue","\n\nimport request from '../utils/request'\n\n// const baseUrl = 'main/magic'\n\nexport function loginFn(data){\n  return request({\n    url:'campus/back_api_login.do',\n    method:'post',\n    data:data\n\n  })\n}\n\n\n\n\n\n\n\n\n\n// WEBPACK FOOTER //\n// ./src/api/login.js","var render = function () {var _vm=this;var _h=_vm.$createElement;var _c=_vm._self._c||_h;return _c('div',[_c('div',{staticClass:\"content\"},[_c('div',{staticClass:\"qrcode\"},[_c('div',{staticClass:\"submit_case\"},[_c('div',{staticClass:\"admin_title\"},[_vm._v(\"54校园V1.0后台管理系统\")]),_vm._v(\" \"),_c('div',{staticClass:\"item\"},[_c('el-radio-group',{staticClass:\"radio_login\",model:{value:(_vm.types),callback:function ($$v) {_vm.types=$$v},expression:\"types\"}},_vm._l((_vm.typeList),function(t){return _c('el-radio',{key:t.value,attrs:{\"label\":t.value}},[_vm._v(_vm._s(t.label))])}),1)],1),_vm._v(\" \"),_c('div',{staticClass:\"item\"},[_c('el-input',{attrs:{\"type\":\"text\",\"placeholder\":\"请输入账号\",\"select-when-unmatched\":true},on:{\"keyup\":function($event){if(!$event.type.indexOf('key')&&$event.keyCode!==13){ return null; }return _vm.loginFn($event)}},nativeOn:{\"keyup\":function($event){if(!$event.type.indexOf('key')&&_vm._k($event.keyCode,\"enter\",13,$event.key,\"Enter\")){ return null; }return _vm.loginFn($event)}},model:{value:(_vm.userName),callback:function ($$v) {_vm.userName=$$v},expression:\"userName\"}},[_c('i',{staticClass:\"el-input__icon iconfont iconaccount\",attrs:{\"slot\":\"prefix\"},slot:\"prefix\"})])],1),_vm._v(\" \"),_c('div',{staticClass:\"item\"},[_c('el-input',{attrs:{\"type\":\"password\",\"placeholder\":\"请输入密码\",\"select-when-unmatched\":true},model:{value:(_vm.pwd),callback:function ($$v) {_vm.pwd=$$v},expression:\"pwd\"}},[_c('i',{staticClass:\"el-input__icon iconfont iconlock\",attrs:{\"slot\":\"prefix\"},slot:\"prefix\"}),_vm._v(\" \"),_c('i',{staticC