.top-header { height: 40px; width:84%; margin: 30px 222px 0 134px; z-index: 9999; position: absolute; top: 0; left: 0;
    .left-logo { float: left; text-align: center; }

  .head-title { float: right; height: 40px; font-size: 16px; cursor: pointer; 
    .tab-item { float: left; padding:0 11px;
        .word-white{
        color: #ffffff; }
      .word-black{
        color: #000000; }
      .lab-part{
        ul{
          font:14px/26px ''; text-align: center;
            li{
            background: #333333; opacity: 0.5; padding: 0 4px; color: #ffffff; }
        }
      }
    }
  }
  .change-white{
    float: right; font-size: 16px; cursor: pointer; color: #ffffff; font-weight: 400; padding-left: 20px; }

  .change-black{
    float: right; font-weight: 400; font-size: 16px; cursor: pointer; color: #000000; padding-left: 20px; }


  .active { color: #e8bc46
  }

}
